{
  "name": "CSharpCornerTagHelpers",
  "private": true,
  "dependencies": {
    "bootstrap": "3.0.0",
    "bootstrap-touch-carousel": "0.8.0",
    "hammer.js": "2.0.4",
    "jquery": "2.1.4",
    "jquery-validation": "1.11.1",
    "jquery-validation-unobtrusive": "3.2.2"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/*.**",
        "dist/css/*.**",
        "dist/fonts/*.**"
      ]
    },
    "bootstrap-touch-carousel": {
      "main": [
        "dist/css/*.**",
        "dist/js/*.**"
      ]
    },
    "hammer.js": {
      "main": "hammer{.js,.min.js,.min.map}"
    },
    "jquery": {
      "main": "dist/jquery{.js,.min.js,.min.map}"
    },
    "jquery-validation": {
      "main": "jquery.validate.js"
    },
    "jquery-validation-unobtrusive": {
      "main": "jquery.validate.unobtrusive{.js,.min.js}"
    }
  }
}
