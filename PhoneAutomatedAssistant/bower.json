{
  "name": "PhoneAutomatedAssistant",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4",
    "jquery-validation": "^1.14.0",
    "jquery-validation-unobtrusive": "^3.2.3"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/*.**",
        "dist/css/*.**",
        "dist/fonts/*.**"
      ]
    },
    "jquery": {
      "main": "dist/jquery.{.js,min.js,.min.map}"
    },
    "jquery-validation-unobtrusive": {
      "main": "jquery.validate.unobtrusive{.js,.min.js}"
    },
    "jquery-validation": {
      "main": [
        "dist/jquery.validate{.js,.min.js}",
        "dist/additional-methods{.js,.min.js}"
      ]
    }
  }
}
