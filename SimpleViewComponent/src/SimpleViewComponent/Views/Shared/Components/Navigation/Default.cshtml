@model SimpleViewComponent.ViewComponents.ViewModel
<div class="navbar-collapse collapse">
    <ul class="nav navbar-nav">
        @foreach (ItemViewModel navigationItem in Model.NavigationItems)
        {
            <li>
                <a asp-controller="@navigationItem.ControllerName" asp-action="@navigationItem.ActionName">@navigationItem.LinkName</a>
            </li>
        }
    </ul>
</div>